  "query": "\n        SELECT\n            sc.id,\n            sc.story_id,\n            sc.user_id,\n            u.username,\n            sc.comment_text,\n            sc.parent_comment_id,\n            sc.reply_count,\n            sc.created_at\n        FROM story_comments sc\n        JOIN users u ON sc.user_id = u.id\n        WHERE sc.story_id = $1 AND sc.parent_comment_id IS NULL\n        ORDER BY sc.created_at ASC\n        ",
