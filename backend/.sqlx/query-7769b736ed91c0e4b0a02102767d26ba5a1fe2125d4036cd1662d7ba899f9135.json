  "query": "\n        SELECT DISTINCT cr.id, cr.name, cr.is_group, cr.created_at, cr.updated_at\n        FROM chat_rooms cr\n        JOIN chat_members cm ON cr.id = cm.chat_room_id\n        WHERE cm.user_id = $1\n        ORDER BY cr.updated_at DESC\n        ",
